CREATE TABLE c1(
	id binary(16),
	data mediumblob
) ENGINE=InnoDB default charset=utf8;

CREATE TABLE c2(
	id binary(16),
	data mediumblob
) ENGINE=InnoDB default charset=utf8;

CREATE TABLE c3(
	id binary(16),
	data mediumblob
) ENGINE=InnoDB default charset=utf8;

CREATE TABLE facts(
	id1 binary(16),	
	id2 binary(16),
	id3 binary(16),
) ENGINE=InnoDB default charset=utf8;

